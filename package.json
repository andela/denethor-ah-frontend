{
  "name": "Denethor-AH-Frontend",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Team Denethor",
  "license": "MIT",
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(css|styl|less|sass|scss)$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.jsx$": "babel-jest",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/test/fileTransformer.js"
    }
  },
  "scripts": {
    "test": "jest -u --coverage",
    "lint": "eslint",
    "serve": "live-server public/",
    "build": "webpack",
    "dev-server": "webpack-dev-server --mode development --hot --open"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.15",
    "@fortawesome/free-solid-svg-icons": "^5.7.2",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "babel-jest": "^24.1.0",
    "coveralls": "^3.0.3",
    "enzyme": "^3.9.0",
    "i": "^0.3.6",
    "jest": "^24.1.0",
    "node-sass": "^4.11.0",
    "normalize.css": "7.0.0",
    "npm": "^6.8.0",
    "prop-types": "^15.7.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-test-renderer": "^16.8.3",
    "sass-loader": "6.0.6",
    "style-loader": "0.18.2",
    "styled-components": "^4.1.3",
    "validator": "^10.11.0",
    "webpack": "^4.28.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "css-loader": "0.28.4",
    "eslint": "^5.14.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.3.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "file-loader": "^3.0.1",
    "identity-obj-proxy": "^3.0.0",
    "live-server": "^1.2.1",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  }
}
