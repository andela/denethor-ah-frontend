{
  "name": "denethor-ah-frontend",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Team Denethor",
  "license": "MIT",
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(css|styl|less|sass|scss)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "raf/polyfill",
      "<rootDir>/enzyme.config.js"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.jsx$": "babel-jest",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/tests/fileTransformer.js"
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverage": true
  },
  "scripts": {
    "build": "webpack --config webpack.prod.babel.js && babel server/index.js -d dist/",
    "dev-server": "webpack-dev-server --config webpack.dev.babel.js --hot",
    "heroku-postbuild": "npm run build",
    "lint": "eslint",
    "serve": "live-server public/",
    "start": "node dist/",
    "test": "jest",
    "test-watch": "jest --watchAll"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.15",
    "@fortawesome/free-brands-svg-icons": "^5.7.2",
    "@fortawesome/free-solid-svg-icons": "^5.7.2",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "babel-jest": "^24.1.0",
    "chalk": "^2.4.2",
    "coveralls": "^3.0.3",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.9.0",
    "enzyme-to-json": "^3.3.5",
    "express": "^4.16.4",
    "i": "^0.3.6",
    "jest": "^24.1.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "normalize.css": "7.0.0",
    "npm": "^6.8.0",
    "prop-types": "^15.7.2",
    "react": "^16.7.0",
    "react-addons-css-transition-group": "^15.6.2",
    "react-dom": "^16.7.0",
    "react-modal": "^3.8.1",
    "react-redux": "^6.0.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-test-renderer": "^16.8.3",
    "react-transition-group": "^2.6.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "sass-loader": "6.0.6",
    "style-loader": "0.18.2",
    "styled-components": "^4.1.3",
    "validator": "^10.11.0",
    "webpack": "^4.28.4",
    "webpack-merge": "^4.2.1",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "css-loader": "0.28.4",
    "eslint": "^5.14.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.3.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "file-loader": "^3.0.1",
    "identity-obj-proxy": "^3.0.0",
    "live-server": "^1.2.1",
    "node-sass": "^4.5.3",
    "polished": "^3.0.3",
    "raf": "^3.4.1",
    "redux-mock-store": "^1.5.3",
    "sass-extract": "^2.1.0",
    "sass-extract-js": "^0.4.0",
    "sass-extract-loader": "^1.1.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  }
}
